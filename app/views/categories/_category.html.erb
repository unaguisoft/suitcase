<tr>
  <td><%= category.name %></td>
  <td><%= category.dimensions %></td>
  <td><%= category.weight %></td>
  <td><%= category.insurance_percent %></td>
  <td><%= category.stock %></td>
  <td class="actions">
    <div class="btn-group">
      <%= link_to 'Edit', [:edit, category], class: 'btn btn-default' %>
      <%= link_to '#', class: 'dropdown-toggle btn btn-default', data: { toggle: 'dropdown' } do %>
        <span class="caret"></span>
        <span class="sr-only">Toggle Dropdown</span>
      <% end %>
      <ul class="dropdown-menu">
          <li>
            <%= link_to 'Delete', category, method: :delete, data: { confirm: 'Are you sure?'}, style: 'color: red' %>
          </li>
      </ul>
    </div>
  </td>
</tr>
